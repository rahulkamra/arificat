		<mx:Label  maxWidth="{this.width *90/100}" alpha="0" text="-{_data.artifact.name} (Lvl {_data.artifactLvl}) " fontSize="12" paddingTop="5"/>
	</mx:Canvas>
	<mx:Script>
		<![CDATA[
			import com.artifact.servermodel.Inventory;
			[Bindable]	
			private var _data:Inventory;
			override public function set data(value:Object):void{
				this._data=value as Inventory;
			}
			
			
		]]>
	</mx:Script>
</mx:Canvas>
